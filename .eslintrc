{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions":{
    "sourceType": "script"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": [2, "never"],
    "prefer-const": 2,
    "no-template-curly-in-string": 2,
    "no-unsafe-negation": 2,
    "curly": [2, "all"],
    "default-case": 2,
    "dot-notation": 2,
    "no-console":"off",
    "no-eq-null": 2,
    "no-else-return": 2,
    "no-empty-function": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-global-assign": 2,
    "no-implied-eval": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-new-wrappers": 2,
    "no-return-assign": 2,
    "no-sequences": 2,
    "no-self-compare": 2,
    "no-throw-literal": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-trailing-spaces": 2
  }
}
